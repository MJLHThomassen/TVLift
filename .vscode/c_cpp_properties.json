{
    "configurations": [
        {
            "name": "ESP32-Win",
            "includePath": [
                "${workspaceRoot}",
                "${workspaceRoot}/**",
                "${env:IDF_PATH}/components/**"
            ],
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceRoot}",
                    "${workspaceRoot}/**",
                    "${env:IDF_PATH}/components"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceRoot}/.vscode/browse.vc.db"
            },
            "cStandard": "c99",
            "cppStandard": "c++17",
            "defines": [ "MG_LOCALS=1" ]
        }
    ],
    "version": 4
}